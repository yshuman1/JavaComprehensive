
public abstract class Account implements Bankable{
	double balance = 1000.00;
	double amount;
	final double INTEREST = .01;
	final double FEES = 1.00;
	public double withdrawal(Account a, double i) {
		amount = i;
		return balance -= amount;
		
	}
	public double deposit(Account a, double i) {
		amount = i;
		return balance += amount;
				
	}
	public double calcInterest() {
	return 0;
	}	
}
