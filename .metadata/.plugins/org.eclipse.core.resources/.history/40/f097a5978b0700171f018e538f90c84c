
public class SomeObject {

private static int count = 10;

String name;
private int i;
SomeObject(String name){
	this.name = name;
	
}
SomeObject(int count) {
	count+= SomeObject.count;
	i++;
	System.out.println("A new object has been created and it is number " + i + " and count is " + count);
}

public static int getCount() {
	// TODO Auto-generated method stub
	return count;
}
public static int updateCount(int count) {
	count ++;
	return count;
	
}



}
